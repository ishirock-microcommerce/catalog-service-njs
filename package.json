    "@types/mongodb": "^3.1.15",
    "@types/mongoose": "^5.3.3",
    "@types/node": "^10.12.10",
    "@types/uuid": "^3.4.4",
    "nodemon": "^1.18.7",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1"
  }
