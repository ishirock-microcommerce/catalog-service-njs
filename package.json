    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "gulp": "^3.9.1",
    "gulp-run": "^1.7.1",
    "gulp-typescript": "^5.0.0",
    "method-override": "^3.0.0",
    "mocha": "^5.2.0",
    "mongodb": "^3.1.10",
    "mongoose": "^5.3.15",
    "swagger-ui-express": "^4.0.1",
    "tsoa": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/express": "^4.16.0",
